{
  "window.density.editorTabHeight": "compact",
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "Kanagawa Dragon",
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#000000",
    "[Evergreen]": {
      "editor.background": "#1E2326"
    }
  },
  "workbench.editor.empty.hint": "hidden",
  "workbench.editorAssociations": {
    "*.csproj": "default"
  },
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "editor.cursorBlinking": "solid",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Iosevka Term','Jetbrains Mono', 'Consolas', 'Courier New', 'monospace'",
  "editor.fontLigatures": "'calt', 'liga', 'dlig'",
  "editor.wordWrap": "on",
  "editor.lineNumbers": "relative",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "editor.lightbulb.enabled": "off",
  "editor.scrollbar.horizontal": "hidden",
  "editor.glyphMargin": true,
  "editor.rulers": [100],
  "editor.tabSize": 2,
  "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "vim.useCtrlKeys": false,
  "vim.smartRelativeLine": true,
  "files.simpleDialog.enable": true,
  "everforest.darkWorkbench": "high-contrast",
  "window.menuBarVisibility": "compact",
  "workbench.layoutControl.enabled": false,
  "vim.showMarksInGutter": true,
  "everforest.darkContrast": "hard",
  "workbench.activityBar.orientation": "vertical"
}
